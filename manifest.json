{
  "manifest_version": 3,
  "name": "IVR Solutions Webrtc Softphone",
  "version": "2.0.0",
  "description": "Professional softphone extension with click-to-call functionality, phone number detection, and call management features",
  "author": "Your Organization",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "action": {
    "default_title": "Click-to-Call Softphone"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "styles.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "scripting",
    "storage",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://founderscartin.s3.ap-south-1.amazonaws.com/app/ivrsolutions/webrtc/chrome-ext/index.html"
  ]
}